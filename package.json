{
  "name": "rehabitter",
  "version": "1.0.0",
  "description": "Helping you replace bad habits with better ones",
  "main": "server.js",
  "scripts": {
    "deploy": "node server",
    "start": "concurrently \"npm run start:*\" \"npm run start:watch:*\"",
    "start:server": "nodemon server",
    "start:watch:tailwind": "onchange tailwind.config.js ./src/css/tailwind.src.css -- npm run build:tailwind",
    "build:tailwind": "postcss build ./src/css/tailwind.src.css -o ./public/css/tailwind.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/geraldiner/rehabitter.git"
  },
  "keywords": [],
  "author": "Geraldine R",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/geraldiner/rehabitter/issues"
  },
  "homepage": "https://github.com/geraldiner/rehabitter#readme",
  "devDependencies": {
    "autoprefixer": "^10.3.4",
    "concurrently": "^6.2.1",
    "nodemon": "^2.0.12",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.1.0",
    "postcss": "^8.3.6",
    "postcss-cli": "^8.3.1",
    "tailwindcss": "^2.2.10"
  },
  "dependencies": {
    "@tailwindcss/custom-forms": "^0.2.1",
    "bcrypt": "^5.0.1",
    "connect-mongo": "^4.5.0",
    "cssnano": "^5.0.8",
    "d3-node": "^2.2.3",
    "dotenv": "^10.0.0",
    "ejs": "^3.1.6",
    "express": "^4.17.1",
    "express-ejs-layouts": "^2.5.1",
    "express-flash": "0.0.2",
    "express-session": "^1.17.2",
    "method-override": "^3.0.0",
    "moment": "^2.29.1",
    "mongoose": "^6.0.5",
    "morgan": "^1.10.0",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "validator": "^13.6.0"
  }
}
